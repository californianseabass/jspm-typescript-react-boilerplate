{
  "name": "jspm-typescript-react-boilerplate",
  "version": "0.0.1",
  "description": "boilerplate to be used for setting up applications run with jspm",
  "main": "server.js",
  "scripts": {
    "bundle": "jspm bundle app.tsx build.js",
    "chokidar": "chokidar-socket-emitter > chokidar.log 2>chokidar-error.log & echo $! > chokidar.pid;",
    "lint": "prettier --config prettier.config.json --write \"**/*.tsx\"",
    "postinstall": "jspm install",
    "start": "jspm run server.tsx",
    "shutdown": "kill -9 $(cat chokidar.pid) && rm chokidar*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "jspm",
    "systemjs",
    "hot",
    "reloading",
    "typescript",
    "react"
  ],
  "author": "Eric Sebastian",
  "license": "ISC",
  "devDependencies": {
    "jspm": "^0.17.0-beta.44"
  },
  "jspm": {
    "name": "jspm-typescript-react-boilerplate",
    "main": "jspm-typescript-react-boilerplate.js",
    "dependencies": {
      "dotenv": "npm:dotenv@^4.0.0",
      "react-dom": "npm:react-dom@^15.6.1"
    },
    "devDependencies": {
      "@babel/core": "npm:@babel/core@^7.0.0-0",
      "babel-core": "npm:babel-core@^7.0.0-07.0.0-0",
      "babel-jest": "npm:babel-jest@^22.4.3",
      "express": "npm:express@^4.15.4",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^7.1.0",
      "querystring": "npm:jspm-nodelibs-querystring@^0.2.0",
      "repl": "npm:jspm-nodelibs-repl@^0.2.0",
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@^1.1.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "typescript": "npm:typescript@^2.4.0"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "react": "npm:react@^15.6.1",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.6.8": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.10": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:pbkdf2@3.0.16": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.2": {
        "browser": "index.js"
      },
      "npm:typescript@2.5.2": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      }
    }
  },
  "dependencies": {
    "express": "^4.15.4"
  }
}
